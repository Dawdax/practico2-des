@model IEnumerable<AP190139_RL190301_Desafio2.DTOs.BitacoraDto>


<h2>Bitácoras</h2>

<table class="table">
    <thead>
        <tr>
            <th>Fecha</th>
            <th>Descripción del Cambio</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var bitacora in Model)
        {
            <tr>
                <td>@bitacora.FechaModificacion.ToString("dd/MM/yyyy HH:mm")</td>
                <td>@bitacora.Modificacion</td>
            </tr>
        }
    </tbody>
</table>

